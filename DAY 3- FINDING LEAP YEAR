#include<stdio.h>
int main()
{
    int yr;
    printf("please enter the year = ");
    scanf("%d",&yr);
    if(yr%4==0)
    {
        if(yr%100==0)
        {
            if(yr%400==0)
            printf("it is a leap year");
            else 
            printf("it is a centrury");            
        }
        else 
        printf("it is a leap year");
    }
    else 
    printf("it is not a leap year");
    return 0;
}
