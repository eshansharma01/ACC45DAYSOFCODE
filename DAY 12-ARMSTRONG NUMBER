#include<stdio.h>  
 int main()    
{    
int n,r,sum=0,s1;    
printf("enter the number=");    
scanf("%d",&n);    
s1=n;    
while(n>0)    
{    
r=n%10;    
sum=sum+(r*r*r);    
n=n/10;    
}    
if(s1==sum)    
printf(" it is a armstrong  number ");    
else    
printf("it's not a armstrong number");    
return 0;  
}   
